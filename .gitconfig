[user]
	email = 7817101+davidchoo12@users.noreply.github.com
	name = David Choo
[core]
	editor = nvim
	excludesfile = ~/.gitignore_global
  attributesfile = ~/.gitattributes_global
[help]
	autocorrect = 1
[pull]
	rebase = false
[pager]
	diff = delta
	log = delta
	reflog = delta
	show = delta
[delta]
	syntax-theme = zenburn
	line-numbers = true
	side-by-side = false
	navigate = true
[interactive]
	diffFilter = delta --color-only
[filter "removeprintln"]
	clean = sed '/println/d'
[push]
	autoSetupRemote = true
[init]
	defaultBranch = master
